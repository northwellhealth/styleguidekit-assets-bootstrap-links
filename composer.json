{
	"name":             "notacouch/styleguidekit-assets-bootstrap-links",
	"description":      "The assets for the default StyleguideKit for Pattern Lab adjusted for Bootstrap. Contains styles and mark-up for Pattern Lab's front-end.",
	"keywords":         ["mustache", "pattern lab", "styleguide", "bootstrap"],
	"homepage":         "http://patternlab.io",
	"license":          "MIT",
	"type":             "patternlab-styleguidekit",
	"replaces": {
		"pattern-lab/styleguidekit-assets-default": "self.version"
	},
	"extra": {
		"patternlab": {
			"dist": {
				"publicDir": [
					{ "*": "*" }
				]
			},
			"config": {
				"ishMinimum": "240",
				"ishMaximum": "2600",
				"ishControlsHide": ["hay"]
			}
		}
	}
}

	"authors": [
		{
			"name":         "Dave Olsen",
			"email":        "dmolsen@gmail.com",
			"homepage":     "http://dmolsen.com",
			"role":         "Lead Developer of original project"
		},
		{
			"name":         "Brad Frost",
			"homepage":     "http://bradfrostweb.com",
			"role":         "Creator of original project"
		},
		{
			"name":			"Abed Islam",
			"role":			"Maintainer of this project"
		}
	],
	"support": {
		"source":         "https://github.com/notacouch/styleguidekit-assets-bootstrap-links"
	},
	"replaces": {
		"pattern-lab/styleguidekit-assets-default": "self.version"
	},
	"extra": {
